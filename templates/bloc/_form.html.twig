{% extends "styles.html.twig" %}

{% block title %}
{% endblock %}

{% block body %}
	<div class="container">


		{{ form_start(form) }}
			<div class="form-group">
				{{ form_label(form.nombloc) }}
				{{ form_widget(form.nombloc) }}
				<div class="form-error">
					{{ form_errors(form.nombloc) }}
				</div>
			</div>

			

			<div class="form-group">
				{{ form_label(form.noteplancher) }}
				{{ form_widget(form.noteplancher) }}
				<div class="form-error">
					{{ form_errors(form.noteplancher) }}
				</div>
			</div>

<div class="form-group">
				{{ form_label(form.filiere) }}
                <br>
				{{ form_widget(form.filiere) }}

			</div>

			<div class="form-group">
				{{ form_label(form.element) }}
                <br>
				{{ form_widget(form.element) }}
				<div class="form-error">
					{{ form_errors(form.element) }}
				</div>
			</div>


		

			
		<br>
 		<button class="btn btn-outline-primary">{{ button_label|default('Enregister') }}</button>
		{{ form_end(form) }}
	</div>
{% endblock %}
