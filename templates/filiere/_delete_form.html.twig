<form method="post" action="{{ path('app_filiere_delete', {'codefiliere': filiere.codefiliere}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ filiere.codefiliere) }}">
    <button class="btn">Delete</button>
</form>
