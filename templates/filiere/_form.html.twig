{% extends "styles.html.twig" %}

{% block title %}
{% endblock %}

{% block body %}
	<div class="container">


					{{ form_start(form) }}


<div class="form-group">
				{{ form_label(form.codefiliere) }}
				{{ form_widget(form.codefiliere) }}
				<div class="form-error">
					{{ form_errors(form.codefiliere) }}
				</div>
			</div>

			<div class="form-group">
				{{ form_label(form.nomfiliere) }}
				{{ form_widget(form.nomfiliere) }}
				<div class="form-error">
					{{ form_errors(form.nomfiliere) }}
				</div>
			</div>

			

			<div class="form-group">
				{{ form_label(form.respfiliere) }}
				{{ form_widget(form.respfiliere) }}
				<div class="form-error">
					{{ form_errors(form.respfiliere) }}
				</div>
			</div>
        
		
			

			<br>
		

			{{ form_row(form._token) }}
 		<button class="btn btn-outline-primary">{{ button_label|default('Enregister') }}</button>
		{{ form_end(form) }}
		</form>
	</div>
{% endblock %}
