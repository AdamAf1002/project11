{% extends 'styles.html.twig' %}

{% block title %}Importer{% endblock %}

{% block body %}
<center>
<h1> Importation d'un fichier csv contenant une maquette </h1>
<br><br><br><br><br>
{{ form_start(form) }}
<div class="form-group">
				{{ form_label(form.annee) }}
				{{ form_widget(form.annee) }}
				<div class="form-error">
					{{ form_errors(form.annee) }}
				</div>
			</div>
<div class="form-group">

	{{ form_label(form.csv_file) }}

				{{ form_widget(form.csv_file) }}
				<div class="form-error">
					{{ form_errors(form.csv_file) }}
				</div><br>
</div>
<button type="submit" class="btn btn-primary">Importer</button>
{{ form_end(form) }}
</center>
   <style>
   .form-control{
    width:fit-content;
       }
   </style>
{% endblock %}
